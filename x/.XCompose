## semi-colon
<dead_horn> <space> : "; "
<dead_horn> <Return> : ";\n"
<dead_horn> <dead_horn> : ";"

## apostrophe and quotes
<dead_horn> <q> : "'"
<dead_horn> <Q> : "\""

## accents 

# acute
<dead_horn> <a> <e> : "é"
<dead_horn> <a> <E> : "É"

# cedilla
<dead_horn> <d> : "ç"
<dead_horn> <D> : "Ç"

# circumflex
<dead_horn> <c> <a> : "â"
<dead_horn> <c> <A> : "Â"
<dead_horn> <c> <e> : "ê"
<dead_horn> <c> <E> : "Ê"
<dead_horn> <c> <i> : "î"
<dead_horn> <c> <I> : "Î"
<dead_horn> <c> <o> : "ô"
<dead_horn> <c> <O> : "Ô"
<dead_horn> <c> <u> : "û"
<dead_horn> <c> <U> : "Û"

# grave
<dead_horn> <g> <a> : "à"
<dead_horn> <g> <A> : "À"
<dead_horn> <g> <e> : "è"
<dead_horn> <g> <E> : "È"
<dead_horn> <g> <u> : "ù"
<dead_horn> <g> <U> : "Ù"


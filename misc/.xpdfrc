#continuousView no
initialSidebarState no
#geometry "1325 1000"
#initialZoom 'page'
#urlCommand "/home/jrm/bin/ck %s"

# key bindings
bind n      any nextPage
bind p      any prevPage
bind ctrl-n any scrollDownNextPage(20)
bind ctrl-p any scrollUpPrevPage(20)
bind alt->  any gotoLastPage
bind alt-<  any gotoPage(1)
bind ctrl-b any scrollLeft(10)
bind ctrl-f any scrollRight(10)
bind ctrl-s any find
bind alt-f1 any toggleOutline
